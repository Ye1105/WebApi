        "type": "string",
        "title": "封面url"
      },
      "blurhash": {
        "type": "string",
        "title": "模糊哈希图像占位符"
      },
      "height": {
        "type": "number",
        "minimum": 0,
        "exclusiveMinimum": true
      },
      "width": {
        "type": "number",
        "minimum": 0,
        "exclusiveMinimum": true
      }
    }
  },
  "register": {
    "type": "object",
    "description": "注册",
    "properties": {
      "phone": {
        "type": "string",
