      "name": {
        "type": "string",
        "title": "账号名"
      },
      "nickName": {
        "type": "string",
        "title": "昵称"
      },
      "pwd": {
        "type": "string",
        "title": "密码",
        "pattern": "^[\\S]{6,12}$"
      }
    },
    "require": [
      "phone",
      "sms",
      "name",
      "nickName",
      "pwd"
    ]
  },
