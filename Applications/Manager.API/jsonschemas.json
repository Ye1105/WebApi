  "blog-add": {
    "type": "object",
    "properties": {
      "sort": {
        "type": "integer",
        "title": "分类",
        "description": "0.公开  1.仅自己可见  2.好友圈  3.粉丝  4.热推  5.广告",
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5
        ],
        "default": 0
      },
      "type": {
        "type": "integer",
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5
        ],
        "default": 1,
        "title": "类型",
        "description": " 0.全部 1.普通文字(表情) 2.头条文章 3.图片 4.音乐 5.视频 "
      },
      "body": {
        "type": "string",
        "title": "内容"
      },
      "images": {
        "title": "图片列表",
        "type": [
          "array",
          "null"
        ],
        "items": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string",
              "title": "相对地址"
            },
            "blurhash": {
              "type": "string",
              "title": "占位符"
            },
            "width": {
              "title": "宽度",
              "type": "number"
            },
            "height": {
              "title": "高度",
              "type": "number"
            }
          },
          "required": [
            "url",
            "blurhash",
            "width",
            "height"
          ]
        },
        "description": "图片对象"
      },
      "video": {
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "title": {
            "type": "string",
            "title": "标题"
          },
          "channel": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "title": "频道"
          },
          "collection": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "title": "集合"
          },
          "type": {
            "title": "类型",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "url": {
            "type": "string",
            "title": "视频地址"
          },
          "duration": {
            "title": "视频时长",
            "type": "number"
          },
          "cover": {
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "url": {
                "type": "string",
                "title": "封面地址"
              },
              "width": {
                "type": "number",
                "title": "宽度"
              },
              "height": {
                "type": "number",
                "title": "高度"
              },
              "blurhash": {
                "type": "string",
                "title": "占位符"
              }
            },
            "required": [
              "url",
              "width",
              "height",
              "blurhash"
            ]
          }
        },
        "required": [
          "title",
          "channel",
          "collection",
          "type",
          "url",
          "duration"
        ],
        "title": "视频对象"
      }
    },
    "required": [
      "sort",
      "type",
      "body"
    ]
  }
}